    "build:app": "cross-env NODE_ENV=production BABEL_ENV=production build-atri-app -d \"#@atrilabs/react-component-manifests\"",
    "build": "npm run build:pyclasses && npm run build:pyapp && npm run build:app",
    "serve": "serve"
  },
  "dependencies": {
